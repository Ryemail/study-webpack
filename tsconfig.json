{
	"compilerOptions": {
		"baseUrl": "./typescript",
		"outDir": "./dist/typescript",
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": false,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"include": ["typescript/**/*"],
	"exclude": ["node_modules"]
}
